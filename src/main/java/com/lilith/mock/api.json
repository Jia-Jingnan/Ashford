  {
    "description": "接口3.列表查询",
    "request": {
      "uri": "/api/member/list",
      "method": "get",
      "cookies": {
        "login": "true"
      }
    },
    "response": {
      "headers": {
        "Content-Type": "text/html;charset=utf-8"
      },
      "json": {
        "name": "stark,thor,loki,steve,banner"
      }
    }
  },
  {
    "description": "接口4.根据user查询用户",
    "request": {
      "uri": "/api/member/user",
      "method": "get",
      "cookies": {
        "login": "true"
      },
      "queries": {
        "name": "Stark",
        "age": "18"
      }
    },
    "response": {
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      },
      "text": "I am IronMan!"
    }
  },
