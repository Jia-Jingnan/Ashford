  {
    "description":"接口2.登陆接口-登陆成功,返回cookie",
    "request":{
      "uri":"/api/member/login",
      "method":"post",
      "json":{
        "phone":"18268046852",
        "password":"123456"
      }
    },
    "response":{
      "status":200,
      "cookies":{
        "login":"true"
      },
      "json": {
        "status": 0,
        "code": "10001",
        "data": null,
        "msg": "登陆成功"
      }
    }
  },
